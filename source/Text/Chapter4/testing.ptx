public class Student {
  	public String name;
  	public int age;
	public int id;

	public Student() {
		name = "No Name";
	} // default constructor

	public static void main(String args[]) {
  		Student student1 = new Student();
  		student1.name = "Jasmine";
  		student1.age = 27;
		Student student2 = new Student();
		student2.name = "Trey";
		student2.age = 20;

	}
}
